<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="loginSQL">
	
	<select id="LoginCheck" parameterType="com.co.kr.common.login.vo.LoginVo" resultType="com.co.kr.common.login.vo.LoginVo">
		SELECT INPUT_ID,
		       FIRST_PWD
		       FROM USER_CREATE_TABLE
		       WHERE INPUT_ID = #{inputId}
		      
		       
		       
	</select>

</mapper>