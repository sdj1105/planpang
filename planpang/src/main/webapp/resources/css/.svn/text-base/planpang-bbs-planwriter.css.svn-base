				background-size: 100% 100%;
				background-position: bottom right;
				z-index:0;
				}
		
		
		.daybox {	display:inline-block;								
					border:5px solid white;
					background-color:rgba(30,60,30,.1);
					width:60px; height:100px;
					font-size:15px;
					text-align:center; 
					overflow:hidden;}	
		.daybox:hover {border:5px solid skyblue;}
		
		.daybox .planbox {	position:absolute;
							width:60px; height:100px;
							border:5px dashed white;
							opacity:0.6;
							}
		
		
		
		
		
		
		
		
		
		
		.emp 		{position:fixed; 
					display:block;
					box-shadow : rgba(0,0,0,0.5) 0 0 0 9999px, rgba(0,0,0,0.5) 2px 2px 3px 3px;				
					z-index : 71;}
		.mask 		{position:fixed; top:0; left:0; 
					display:block;
					width:100%; height:100%;
					z-index: 70;}
					
		.insertemp {position:fixed; 
					display:block;
					box-shadow : rgba(0,0,0,0.5) 0 0 0 9999px, rgba(0,0,0,0.5) 2px 2px 3px 3px;				
					z-index: 81;}
		.insertmask {position:fixed; top:0; left:0; 
					display:block;
					width:100%; height:100%;
					z-index: 80;}			
		
					
		.onmouse-content{display:none; position:absolute; top:0px; left:0px;
						border:0px solid; border-radius:15px; 
